<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>ReSharper.XmlDocInspections</id>
    <version>$PackageVersion$</version>
    <title>XML Doc Inspections</title>
    <authors>ulrichb</authors>
    <license type="expression">MIT</license>
    <projectUrl>https://github.com/ulrichb/XmlDocInspections#readme</projectUrl>
    <iconUrl>https://raw.githubusercontent.com/ulrichb/XmlDocInspections/master/Doc/Icons/Xml128.png</iconUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Configurable warnings for missing XML Doc comments</description>
    <releaseNotes>$PackageReleaseNotes$</releaseNotes>
    <tags>xml doc xmldoc inspection warning</tags>
